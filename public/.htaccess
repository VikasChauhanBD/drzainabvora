<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /

  # üîÅ Redirect www to non-www (SEO 301)
  RewriteCond %{HTTP_HOST} ^www\.drzainabvora\.com$ [NC]
  RewriteRule ^(.*)$ https://drzainabvora.com/$1 [L,R=301]

  # ‚öõÔ∏è React SPA routing
  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /index.html [L]
</IfModule>
